<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document>
    <name>Pune Zones</name>
    
    <!-- Define a Style for red boundaries -->
    <Style id="redBorder">
      <LineStyle>
        <color>ff0000ff</color>  <!-- Red color (AABBGGRR format) -->
        <width>3</width>         <!-- Border thickness -->
      </LineStyle>
      <PolyStyle>
        <color>4d0000ff</color>  <!-- Semi-transparent red fill (optional) -->
        <fill>1</fill>           <!-- Fill enabled -->
        <outline>1</outline>     <!-- Show outline -->
      </PolyStyle>
    </Style>

    <!-- Apply the style to each Placemark -->
    <Placemark>
      <name>Shivajinagar</name>
      <styleUrl>#redBorder</styleUrl>  <!-- Reference the style -->
      <Polygon>
        <outerBoundaryIs>
          <LinearRing>
            <coordinates>
              73.8432,18.5325,0
              73.8465,18.5322,0 
              73.8470,18.5350,0 
              73.8435,18.5353,0
              73.8432,18.5325,0
            </coordinates>
          </LinearRing>
        </outerBoundaryIs>
      </Polygon>
    </Placemark>
  </Document>
</kml>
