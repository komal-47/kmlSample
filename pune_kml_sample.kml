<?xml version="1.0" encoding="UTF-8"?>
<kml xmlns="http://www.opengis.net/kml/2.2">
  <Document>
    <name>Godhpadi Buildings (Visible Boundaries)</name>
       <name>Sai Apartments</name>
    <!-- Style for RED BOUNDARIES -->
    <Style id="redBuildingStyle">
      <LineStyle>
        <color>ff0000ff</color>  <!-- Solid red border -->
        <width>4</width>        <!-- Thicker border -->
      </LineStyle>
      <PolyStyle>
        <color>33ff0000</color>  <!-- Semi-transparent red fill -->
        <fill>1</fill>          <!-- Enable fill -->
      </PolyStyle>
    </Style>

    <!-- ===== BUILDING 1: Sai Apartments (Larger Polygon) ===== -->
    <Placemark>
      <name>Sai Apartments</name>
      <styleUrl>#redBuildingStyle</styleUrl>
      <Polygon>
        <outerBoundaryIs>
          <LinearRing>
            <coordinates>
              73.9070,18.5920,0
              73.9080,18.5920,0  <!-- Wider span (~100m) -->
              73.9080,18.5930,0
              73.9070,18.5930,0
              73.9070,18.5920,0  <!-- Close the polygon -->
            </coordinates>
          </LinearRing>
        </outerBoundaryIs>
      </Polygon>
    </Placemark>

    <!-- ===== BUILDING 2: Godhpadi Heights ===== -->
    <Placemark>
      <name>Godhpadi Heights</name>
      <styleUrl>#redBuildingStyle</styleUrl>
      <Polygon>
        <outerBoundaryIs>
          <LinearRing>
            <coordinates>
              73.9085,18.5910,0
              73.9095,18.5910,0
              73.9095,18.5920,0
              73.9085,18.5920,0
              73.9085,18.5910,0
            </coordinates>
          </LinearRing>
        </outerBoundaryIs>
      </Polygon>
    </Placemark>
  </Document>
</kml>
